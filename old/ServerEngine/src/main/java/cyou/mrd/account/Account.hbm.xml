<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cyou.mrd.account">
    <class name="Account" table="account">
        <id name="id" column="id">
			<generator class="native" />
		</id>
        <property name="mid" type="string"/>
        <property name="imoney" type="int"/>
        <property name="compensateDollar" type="int"/>
        <property name="rewardDollar" type="int"/>
        <property name="usedDollar" type="int"/>
        <property name="initDollar" type="int"/>
        <property name="createTime" type="timestamp" column="createTime" /> 
        <property name="lastLoginTime" type="timestamp" column="lastLoginTime" /> 
    </class>
	
</hibernate-mapping>